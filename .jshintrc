{
	// Default options
	"boss": true,
	"browser": true,
	"curly": true,
	"eqeqeq": true,
	"eqnull": true,
	"expr": true,
	"immed": true,
	"indent": 4,
	"latedef": "nofunc",
	"newcap": true,
	"noarg": true,
	"sub": true,
	"trailing": true,
	"undef": true,
	"unused": true,
	"globals": {
