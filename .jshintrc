		"$": true
	},

	// Project options: Default off
	"camelcase": true,
	"forin": true,
	"quotmark": "single",
	"strict": true,

	// Project options: Default on
	"boss": false,

	// Other Project options:
	"maxlen": 100,
	"globals": {
		"jQuery": true,
		"ace": true
	}
}
