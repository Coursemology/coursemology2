{
  "name": "coursemology",
  "version": "2.0.0",
  "description": "Coursemology Frontend",
  "engines": {
    "node": ">=5.10.0",
    "yarn": "^1.0.0"
  },
  "scripts": {
    "test": "TZ=Asia/Singapore yarn run jest --coverage",
    "testci": "TZ=Asia/Singapore yarn run jest --maxWorkers=4 --coverage",
    "build:test": "export NODE_ENV=test && yarn run build:translations && webpack -pw",
    "build:production": "export NODE_ENV=production && yarn run build:translations && webpack -p",
    "build:development": "yarn run build:translations && webpack-dev-server",
    "build:translations": "babel-node scripts/aggregate-translations.js",
    "extract-translations": "yarn run build:production && babel-node scripts/extract-translations.js",
    "lint-src": "eslint . --ext .js --ext .jsx --cache --ignore-pattern '**/__test__/**' --ignore-pattern 'coverage/**'",
    "lint-tests": "eslint . --ext .test.js --ext .test.jsx --cache -c .eslintrc.test",
    "lint": "yarn run lint-src && yarn run lint-tests && prettier --check \"**/*.{js,jsx}\"",
    "lint-fix": "yarn run lint-src --fix && yarn run lint-tests --fix && prettier --write \"**/*.{js,jsx}\"",
    "postinstall": "cd vendor/recorderjs && NODE_ENV=development yarn install"
  },
  "cacheDirectories": [
    "node_modules",
    "client/node_modules"
  ],
  "jest": {
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest"
    },
    "moduleDirectories": [
      "node_modules",
      "app"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ],
    "setupFiles": [
      "<rootDir>/app/__test__/requestAnimationFrame.js",
      "jest-localstorage-mock"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/app/__test__/setup.js"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      ".scss$": "<rootDir>/SassStub.js",
      "^api(.*)$": "<rootDir>/app/api$1",
      "^lib(.*)$": "<rootDir>/app/lib$1",
      "^utils(.*)$": "<rootDir>/app/__test__/utils$1",
      "^course(.*)$": "<rootDir>/app/bundles/course$1"
    },
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/__test__/"
    ]
  },
  "dependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.15.3",
    "@date-io/moment": "^1.3.6",
    "@material-ui/core": "^3.8.3",
    "ace-builds": "^1.4.13",
    "axios": "^0.21.2",
    "babel-loader": "^8.0.6",
    "babel-plugin-react-intl": "^3.1.1",
    "chart.js": "^2.8.0",
    "css-loader": "^1.0.0",
    "expose-loader": "^0.7.5",
    "fabric": "5.0.0",
    "glob": "^7.1.4",
    "hard-source-webpack-plugin": "^0.13.1",
    "history": "^4.9.0",
    "immutable": "^3.8.1",
    "intl": "^1.2.5",
    "jquery": "3.4.1",
    "jquery-ui": "^1.12.1",
    "json-loader": "^0.5.4",
    "material-ui": "^0.20.2",
    "material-ui-chip-input": "^0.19.0",
    "material-ui-pickers": "^2.2.4",
    "mirror-creator": "1.1.0",
    "mkdirp": "^0.5.1",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.25",
    "node-sass": "^4.12.0",
    "prop-types": "^15.7.2",
    "rc-slider": "^8.6.11",
    "react": "^16.8.6",
    "react-ace": "^9.5.0",
    "react-chartjs-2": "^2.7.6",
    "react-color": "^2.17.3",
    "react-dnd": "^5.0.0",
    "react-dnd-html5-backend": "^5.0.1",
    "react-dom": "^16.8.6",
    "react-dropzone": "^5.0.1",
    "react-hot-keys": "^2.6.2",
    "react-intl": "^2.9.0",
    "react-overlays": "^0.8.3",
    "react-player": "^1.11.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scroll": "^1.7.11",
    "react-summernote": "github:ekowidianto/react-summernote",
    "react-tooltip": "^3.10.0",
    "redux": "^4.0.1",
    "redux-form": "^7.4.2",
    "redux-immutable": "^4.0.0",
    "redux-persist": "^5.10.0",
    "redux-promise": "^0.6.0",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "sass-loader": "^7.1.0",
    "script-loader": "^0.7.0",
    "style-loader": "^0.23.1",
    "webfontloader": "^1.6.27",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2",
    "webpack-manifest-plugin": "^2.0.4"
  },
  "devDependencies": {
    "axios-mock-adapter": "^1.16.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.13.2",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-import-resolver-webpack": "^0.13.1",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.25.1",
    "jest": "^24.8.0",
    "jest-localstorage-mock": "2.4.0",
    "prettier": "^2.4.0",
    "react-dnd-test-backend": "^7.4.4",
    "react-test-renderer": "^16.8.6",
    "redux-logger": "^3.0.6",
    "webpack-dev-server": "^3.4.1"
  },
  "license": "MIT",
  "repository": "git+https://github.com/Coursemology/coursemology2.git",
  "main": "app/index.js"
}
