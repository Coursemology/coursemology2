{
  "name": "coursemology",
  "version": "2.0.0",
  "description": "Coursemology Frontend",
  "engines": {
    "node": ">=5.10.0",
    "yarn": "^1.0.0"
  },
  "scripts": {
    "test": "yarn run jest --coverage",
    "build:test": "export NODE_ENV=test && yarn run build:translations && webpack -w",
    "build:production": "export NODE_ENV=production && yarn run build:translations && webpack -p",
    "build:development": "yarn run build:translations && webpack-dev-server",
    "build:translations": "babel-node scripts/aggregate-translations.js",
    "extract-translations": "yarn run build:production && babel-node scripts/extract-translations.js",
    "lint-src": "eslint . --ext .js --ext .jsx --cache --ignore-pattern '**/__test__/**' --ignore-pattern 'coverage/**'",
    "lint-tests": "eslint . --ext .test.js --ext .test.jsx --cache -c .eslintrc.test",
    "lint": "yarn run lint-src && yarn run lint-tests",
    "lint-fix": "yarn run lint-src --fix && yarn run lint-tests --fix",
    "postinstall": "cd vendor/recorderjs && NODE_ENV=development yarn install"
  },
  "cacheDirectories": [
    "node_modules",
    "client/node_modules"
  ],
  "jest": {
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest"
    },
    "moduleDirectories": [
      "node_modules",
      "app"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ],
    "setupFiles": [
      "<rootDir>/app/__test__/requestAnimationFrame.js",
      "jest-localstorage-mock"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/app/__test__/setup.js",
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      ".scss$": "<rootDir>/SassStub.js",
      "^api(.*)$": "<rootDir>/app/api$1",
      "^lib(.*)$": "<rootDir>/app/lib$1",
      "^utils(.*)$": "<rootDir>/app/__test__/utils$1",
      "^course(.*)$": "<rootDir>/app/bundles/course$1"
    },
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/__test__/"
    ]
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel": "^6.5.2",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.1.4",
    "babel-plugin-react-intl": "^2.4.0",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-runtime": "^6.6.1",
    "brace": "^0.11.1",
    "chart.js": "^2.7.2",
    "css-loader": "^0.28.11",
    "enzyme-adapter-react-16": "^1.1.1",
    "expose-loader": "^0.7.5",
    "fabric": "2.2.2",
    "glob": "^7.1.2",
    "hard-source-webpack-plugin": "^0.6.9",
    "history": "^4.6.1",
    "immutable": "^3.8.1",
    "intl": "^1.2.5",
    "jquery": "3.3.1",
    "jquery-ui": "^1.12.1",
    "json-loader": "^0.5.4",
    "material-ui": "^0.20.0",
    "material-ui-chip-input": "^0.18.7",
    "mirror-creator": "1.1.0",
    "mkdirp": "^0.5.1",
    "moment": "^2.21.0",
    "moment-timezone": "^0.5.13",
    "node-sass": "^4.7.2",
    "prop-types": "^15.6.1",
    "rc-slider": "^8.6.1",
    "react": "^16.2.0",
    "react-ace": "^5.9.0",
    "react-chartjs-2": "^2.7.0",
    "react-color": "^2.14.0",
    "react-dnd": "^2.4.0",
    "react-dnd-html5-backend": "^2.4.1",
    "react-dom": "^16.2.0",
    "react-dropzone": "^4.2.9",
    "react-intl": "^2.3.0",
    "react-overlays": "^0.8.3",
    "react-player": "^1.3.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-scroll": "^1.7.7",
    "react-summernote": "^2.0.0",
    "react-tooltip": "^3.3.0",
    "redux": "^3.3.1",
    "redux-form": "^7.3.0",
    "redux-immutable": "^4.0.0",
    "redux-persist": "^5.9.1",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.0.1",
    "reselect": "^3.0.1",
    "sass-loader": "^6.0.7",
    "script-loader": "^0.7.0",
    "style-loader": "^0.20.3",
    "webfontloader": "^1.6.27",
    "webpack": "^3.10.0",
    "webpack-manifest-plugin": "^1.3.1"
  },
  "devDependencies": {
    "axios-mock-adapter": "^1.14.1",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^21.2.0",
    "enzyme": "^3.3.0",
    "enzyme-to-json": "^3.3.3",
    "eslint": "^4.19.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-import-resolver-webpack": "^0.8.4",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "jest": "^21.2.1",
    "jest-localstorage-mock": "^2.2.0",
    "react-dnd-test-backend": "^2.4.0",
    "react-test-renderer": "^16.2.0",
    "redux-logger": "^3.0.6",
    "webpack-dev-server": "^2.11.1"
  },
  "license": "MIT",
  "repository": "git+https://github.com/Coursemology/coursemology2.git",
  "main": "app/index.js"
}
