{
  "name": "coursemology",
  "version": "2.0.0",
  "description": "Coursemology Frontend",
  "engines": {
    "node": ">=5.10.0",
    "yarn": "^1.0.0"
  },
  "scripts": {
    "test": "yarn run jest --coverage",
    "build:test": "export NODE_ENV=test && yarn run build:translations && webpack -w",
    "build:production": "export NODE_ENV=production && yarn run build:translations && webpack -p",
    "build:development": "yarn run build:translations && webpack-dev-server",
    "build:translations": "babel-node scripts/aggregate-translations.js",
    "extract-translations": "yarn run build:production && babel-node scripts/extract-translations.js",
    "lint-src": "eslint . --ext .js --ext .jsx --cache --ignore-pattern '**/__test__/**' --ignore-pattern 'coverage/**'",
    "lint-tests": "eslint . --ext .test.js --ext .test.jsx --cache -c .eslintrc.test",
    "lint": "yarn run lint-src && yarn run lint-tests",
    "lint-fix": "yarn run lint-src --fix && yarn run lint-tests --fix",
    "postinstall": "cd vendor/recorderjs && NODE_ENV=development yarn install"
  },
  "cacheDirectories": [
    "node_modules",
    "client/node_modules"
  ],
  "jest": {
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest"
    },
    "moduleDirectories": [
      "node_modules",
      "app"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/app/__test__/setup.js",
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "^api(.*)$": "<rootDir>/app/api$1",
      "^lib(.*)$": "<rootDir>/app/lib$1",
      "^utils(.*)$": "<rootDir>/app/__test__/utils$1",
      "^course(.*)$": "<rootDir>/app/bundles/course$1",
      ".scss$": "<rootDir>/SassStub.js"
    },
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/__test__/"
    ]
  },
  "dependencies": {
    "@types/moment-timezone": "^0.2.34",
    "axios": "^0.16.1",
    "babel": "^6.5.2",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-react-intl": "^2.2.0",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-runtime": "^6.6.1",
    "brace": "^0.10.0",
    "css-loader": "^0.28.2",
    "expose-loader": "^0.7.3",
    "fabric": "1.7.17",
    "glob": "^7.1.2",
    "history": "^4.6.1",
    "immutable": "^3.8.1",
    "intl": "^1.2.5",
    "jquery": "2.2.4",
    "jquery-ui": "^1.12.1",
    "json-loader": "^0.5.4",
    "material-ui": "^0.18.1",
    "mirror-creator": "1.1.0",
    "mkdirp": "^0.5.1",
    "moment": "^2.18.1",
    "moment-timezone": "^0.5.13",
    "node-sass": "^4.5.3",
    "prop-types": "^15.5.10",
    "rc-slider": "^8.3.1",
    "react": "^15.5.4",
    "react-ace": "^4.3.0",
    "react-color": "^2.13.1",
    "react-dnd": "^2.4.0",
    "react-dnd-html5-backend": "^2.4.1",
    "react-dom": "^15.5.4",
    "react-dropzone": "^3.13.2",
    "react-intl": "^2.3.0",
    "react-overlays": "^0.7.0",
    "react-paginate": "^5.0.0",
    "react-player": "^0.24.2",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-scroll": "^1.5.2",
    "react-summernote": "^2.0.0",
    "react-tap-event-plugin": "^2.0.1",
    "react-tooltip": "^3.3.0",
    "redux": "^3.3.1",
    "redux-form": "^6.7.0",
    "redux-immutable": "^4.0.0",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.0.1",
    "sass-loader": "^6.0.5",
    "script-loader": "^0.7.0",
    "style-loader": "^0.18.0",
    "webfontloader": "^1.6.27",
    "webpack": "^2.5.1",
    "webpack-manifest-plugin": "^1.3.1"
  },
  "devDependencies": {
    "axios-mock-adapter": "^1.8.1",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "enzyme": "^2.8.2",
    "enzyme-to-json": "^1.5.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-import": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "jest": "^20.0.3",
    "react-dnd-test-backend": "^2.4.0",
    "react-test-renderer": "^15.5.4",
    "redux-logger": "^3.0.6",
    "webpack-dev-server": "^2.4.5"
  },
  "license": "MIT",
  "repository": "git+https://github.com/Coursemology/coursemology2.git",
  "main": "app/index.js"
}
