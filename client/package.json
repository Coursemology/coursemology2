{
  "name": "coursemology",
  "version": "2.0.0",
  "description": "Coursemology Frontend",
  "engines": {
    "node": ">=5.10.0",
    "yarn": "^1.0.0"
  },
  "scripts": {
    "test": "TZ=Asia/Singapore yarn run jest --coverage",
    "testci": "TZ=Asia/Singapore yarn run jest --maxWorkers=4 --coverage",
    "build:test": "export NODE_ENV=test && yarn run build:translations && webpack -w --node-env=production",
    "build:production": "export NODE_ENV=production && yarn run build:translations && webpack --node-env=production",
    "build:development": "yarn run build:translations && webpack serve",
    "build:translations": "babel-node scripts/aggregate-translations.js",
    "extract-translations": "yarn run build:production && babel-node scripts/extract-translations.js",
    "lint-src": "eslint . --ext .js --ext .jsx --ext .ts --ext .tsx --cache --ignore-pattern '**/__test__/**' --ignore-pattern 'coverage/**'",
    "lint-tests": "eslint . --ext .test.js --ext .test.jsx --ext .test.ts --ext .test.tsx --cache",
    "lint": "yarn run lint-src && yarn run lint-tests && prettier --check \"**/*.{js,jsx,ts,tsx}\"",
    "lint-fix": "yarn run lint-src --fix && yarn run lint-tests --fix && prettier --write \"**/*.{js,jsx,ts,tsx}\"",
    "postinstall": "cd vendor/recorderjs && NODE_ENV=development yarn install"
  },
  "cacheDirectories": [
    "node_modules",
    "client/node_modules"
  ],
  "jest": {
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest"
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!intl-messageformat|intl-messageformat-parser).+\\.js$"
    ],
    "moduleDirectories": [
      "node_modules",
      "app"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx",
      "json"
    ],
    "setupFiles": [
      "<rootDir>/app/__test__/requestAnimationFrame.js",
      "jest-localstorage-mock"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/app/__test__/setup.js"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      ".scss$": "<rootDir>/SassStub.js",
      "^api(.*)$": "<rootDir>/app/api$1",
      "^lib(.*)$": "<rootDir>/app/lib$1",
      "^theme(.*)$": "<rootDir>/app/theme$1",
      "^testUtils(.*)$": "<rootDir>/app/__test__/utils$1",
      "^course(.*)$": "<rootDir>/app/bundles/course$1"
    },
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/__test__/"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "dependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.18.10",
    "@babel/node": "^7.18.10",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-react": "^7.18.6",
    "@babel/runtime": "^7.18.9",
    "@ckeditor/ckeditor5-build-custom": "github:ekowidianto/CKEditor5-build-coursemology#v1",
    "@ckeditor/ckeditor5-react": "^5.0.2",
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@hookform/resolvers": "^2.9.7",
    "@mui/icons-material": "^5.8.4",
    "@mui/lab": "^5.0.0-alpha.95",
    "@mui/material": "^5.10.1",
    "@mui/styles": "^5.9.3",
    "@mui/x-date-pickers": "^5.0.0-beta.6",
    "ace-builds": "^1.9.6",
    "axios": "^0.27.2",
    "babel-loader": "^8.2.5",
    "babel-plugin-formatjs": "^10.3.25",
    "chart.js": "^3.8.2",
    "css-loader": "^6.7.1",
    "dnd-core": "^14.0.1",
    "expose-loader": "^4.0.0",
    "fabric": "^5.2.1",
    "fast-deep-equal": "^3.1.3",
    "glob": "^8.0.3",
    "hard-source-webpack-plugin": "^0.13.1",
    "history": "^5.2.0",
    "immer": "^9.0.15",
    "immutable": "^3.8.2",
    "intl": "^1.2.5",
    "jquery": "^3.6.0",
    "jquery-ui": "^1.13.2",
    "json-loader": "^0.5.7",
    "lodash": "^4.17.21",
    "mirror-creator": "1.1.0",
