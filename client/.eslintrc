{
  "extends": "airbnb",
  "settings": {
    import/resolver: "webpack"
  },
  "rules": {
    # See https://github.com/yannickcr/eslint-plugin-react/issues/847
    "react/no-unused-prop-types": ["warn", { "skipShapeProps": true }]
  },
  "globals": {
    "window": true,
    "document": true,
    "$": true,
    "FormData": true,
  }
}
